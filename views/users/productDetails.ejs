                                        <input type="hidden" name="selectedSize" id="selectedSize" value="s" required>

                                        <script>
                                            // Ensure that one of the radio buttons is selected by default
                                            document.querySelector('#sizeButtons input:checked').parentNode.classList.add('selected');

                                            function selectSize(radioButton, size) {
                                                // Remove 'selected' class from all labels
                                                document.querySelectorAll('#sizeButtons label').forEach(label => {
                                                    label.classList.remove('selected');
                                                });

                                                // Highlight the label of the selected radio button
                                                radioButton.parentNode.classList.add('selected');

                                                // Update the hidden input field with the selected size
                                                document.getElementById('selectedSize').value = size;
                                            }
                                        </script>


