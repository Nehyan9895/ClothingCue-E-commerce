        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <% if (cart && cart.length> 0) { %>
                            <% var totalAmount=0; %>

                                <div class="table-responsive">
                                    <table class="table shopping-summery text-center clean">
                                        <thead>
                                            <tr class="main-heading">
                                                <th scope="col">Image</th>
                                                <th scope="col">Name</th>
                                                <th scope="col">Price</th>
                                                <th scope="col">Quantity</th>
                                                <th scope="col">Subtotal</th>
                                                <th scope="col">Remove</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for (let i=0; i < cart.length; i++) { %>
                                                <% for(let j=0;j<cart[i].products.length;j++){ %>
                                                    <input type="hidden" name="productId"
                                                        value="<%= cart[i].products[j].productId  %>" id="">

                                                    <tr>
                                                        <td class="image product-thumbnail"><img
                                                                src="/<%=cart[i].products[j].productId.images[0]%>"
                                                                alt="#"></td>
                                                        <td class="product-des product-name">
                                                            <h5 class="product-name"><a
                                                                    href="/productdetails/<%=cart[i].products[j].productId%>">
                                                                    <%= cart[i].products[j].productId.name %>

                                                                </a>
                                                                <p>
                                                                    <% if (cart[i].products[j].size==='s' ) { %>
                                                                        small
                                                                        <% } else if(cart[i].products[j].size==='m' ) {
                                                                            %>
                                                                            medium
                                                                            <% } else if (cart[i].products[j].size==='l'
                                                                                ) { %>
                                                                                large
                                                                                <% } %>
                                                                </p>
                                                                </p>
                                                               

                                                            </h5>

                                                        </td>
                                                        <% const product=cart[i].products[j]; %>
                                                            <% const price=cart[i].products[j].productId.salesPrice %>
                                                                <% const subtotal=parseFloat(product.quantity * price);
                                                                    %>
                                                                    <% var totalAmount=totalAmount + subtotal; %>
                                                                        <td class="price" data-title="Price">₹<span
                                                                                id="price-<%=i%>" class="product-price">
                                                                                <%= price%>
                                                                            </span></td>
                                                                        <td class="text-center" data-title="Stock">
                                                                            <div
                                                                                class="detail-qty border radius  m-auto">
                                                                                <a class="qty-down"
                                                                                    onclick="decreaseQuantity(this, `<%=i%>`, `<%= cart[i]._id %>`, `<%= cart[i].products[j]._id%>`)">
                                                                                    <i
                                                                                        class="fi-rs-angle-small-down"></i>
                                                                                </a>
                                                                                <span class="qty-val"
                                                                                    id="qty-val_<%= cart[i].products[j]._id %>">
                                                                                    <%= cart[i].products[j].quantity %>
                                                                                </span>
                                                                                <a href="#" class="qty-up"
                                                                                    onclick="increaseQuantity(this, `<%=i%>`, `<%= cart[i]._id %>`, `<%= cart[i].products[j]._id %>`)">
                                                                                    <i class="fi-rs-angle-small-up"></i>
                                                                                </a>
                                                                            </div>

                                                                        </td>
                                                                        <td class="text-right" data-title="Cart">
                                                                            ₹<span class="subtotal"
                                                                                id="subtotal-<%=i%>">
                                                                                <%=subtotal%>
                                                                            </span>
                                                                        </td>

                                                                        <td class="action" data-title="Remove"><a
                                                                                href="#" class="text-muted"
                                                                                onclick="confirmRemoveCartItem(`<%=cart[i]._id%>`,`<%=cart[i].products[j]._id %>`)"><i
                                                                                    class="fi-rs-trash"></i></a></td>
                                                    </tr>
                                                    <% } %>
                                                        <% } %>



                                        </tbody>
                                    </table>
                                    <% } else { %>
                                        <h4>Your cart is empty!</h4>
                                        <%}%>
                                            <script>

                                                function increaseQuantity(element, i, cartId, productId) {
                                                    const quantityElement = document.getElementById('qty-val_' + productId);
                                                    const currentQuantity = parseInt(quantityElement.innerHTML);

                                                    // Check if current quantity is less than the maximum (8)
                                                    if (currentQuantity < 5) {
                                                        const newQuantity = currentQuantity + 1;
                                                        quantityElement.innerHTML = newQuantity;

                                                        // Perform any additional logic, e.g., update the quantity in the server or trigger an event
                                                        updateQuantity(cartId, newQuantity, productId);

                                                        // Disable the button if the maximum quantity is reached
                                                        if (newQuantity === 5) {
                                                            element.disabled = true;
                                                        }
                                                    } else {
                                                        location.reload();
                                                        alert('Maximum quantity reached!');
                                                    }
                                                }



                                                function decreaseQuantity(element, i, cartId, productId) {
                                                    const quantityElement = element.parentElement.querySelector('.qty-val');
                                                    const currentQuantity = parseInt(quantityElement.innerText, 10);
                                                    const newQuantity = Math.max(currentQuantity - 1, 1);
                                                    quantityElement.innerText = newQuantity;
                                                    updateQuantity(cartId, newQuantity, productId);
                                                }


                                                function updateQuantity(cartId, newQuantity, productId) {
                                                    fetch('/updateQuantity', {
                                                        method: 'PUT',
                                                        headers: {
                                                            'Content-Type': 'application/json',
                                                        },
                                                        body: JSON.stringify({
                                                            cartId: cartId,
                                                            newQuantity: newQuantity,
                                                            productId: productId,
                                                        }),
                                                    })
                                                        .then((response) => {
                                                            if (!response.ok) {
                                                                throw new Error(`HTTP error! Status: ${response.status}`);
                                                            }

                                                            // Use the provided code to handle different content types
                                                            const contentType = response.headers.get('content-type');
                                                            if (contentType && contentType.includes('application/json')) {
                                                                // If the content type is JSON, parse the response as JSON
                                                                return response.json();
                                                            } else {
                                                                // If the content type is not JSON, handle it as plain text
                                                                return response.text();
                                                            }
                                                        })
                                                        .then((data) => {
                                                            // Handle successful response from the server
                                                            console.log('Quantity updated successfully:', data);
                                                            location.reload()
                                                            // Perform UI update if needed
                                                        })
                                                        .catch((error) => {
                                                            // Handle error
                                                            console.error('There was a problem updating quantity:', error);
                                                        });
                                                }

                                            </script>

                                </div>
                                <div class="cart-action text-end">
                                    <a class="btn" href="/home"><i class="fi-rs-shopping-bag mr-10"></i>Continue
