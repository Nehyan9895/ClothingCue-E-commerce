    <script>
        async function confirmRemoveCartItem(cartId, productId) {
            const confirmed = await Swal.fire({
                title: "Are you sure?",
                text: "You won't be able to revert this!",
                icon: "warning",
                showCancelButton: true,
                confirmButtonColor: "#3085d6",
                cancelButtonColor: "#d33",
                confirmButtonText: "Yes, delete it!",
            });

            if (confirmed.isConfirmed) {
                removeCartItem(cartId, productId);
            } else {
                console.log("Cancelled");
            }
        }

        function removeCartItem(cartId, productId) {
            fetch(`/removeCartItem?cartId=${cartId}&productId=${productId}`, {
                method: 'GET',
                redirect: 'follow',
            })
                .then(response => {
                    const contentType = response.headers.get('content-type');

                    if (contentType && contentType.includes('application/json')) {
                        return response.json();
                    } else {
                        console.log('Received HTML response:', response);

                        // Assuming that a successful removal will also redirect to the /cart page
                        if (response.url.endsWith('/cart')) {
                            localStorage.setItem('productRemoved', 'true');
                            // Redirect to the desired page after successful removal
                            window.location.href = '/cart';
                        } else {
                            console.error('Unexpected HTML response. Product may not be removed.');
                        }

                        return Promise.reject(new Error('HTML response'));
                    }
                })
                .catch(error => {
                    console.log('Error in fetch:', error.message);
                });
        }




    </script>



