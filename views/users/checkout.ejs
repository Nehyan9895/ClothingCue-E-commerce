                    <a type="submit" href="/addressform" class="btn btn-small button-add-to-cart col-md-2 mb-10">Add New
                        Address</a>
                    </div>
                
                <div class="row">

                    <div class=" ms-5 col-md-5 ">

                        <%if(user.address.length>0){%>
                            <form action="/account" id="orderForm" method="post">
                                <%for(let i=0;i<user.address.length;i++){%>
                                    <div class="card col-md-3 ml-3 mr-3" style="width: 18rem; height: fit-content;">
                                        <div class="card-body">
                                            <h5 class="card-title">
                                                <%=user.address[i].addressName%>
                                            </h5>
                                            <h6 class="card-subtitle mb-2 text-muted">
                                                <%=user.address[i].house%>
                                            </h6>
                                            <p class="card-text">Name : <%=user.address[i].fname%>
                                            </p>
                                            <p class="card-text">City : <%=user.address[i].city%>
                                            </p>
                                            <p class="card-text">Phone : <%=user.address[i].phone%>
                                            </p>
                                            <p class="card-text">Landmark : <%=user.address[i].landmark%>
                                            </p>
                                            <p class="card-text">Pincode : <%=user.address[i].pincode%>
                                            </p>
                                            <a href="/editaddress?i=<%=i%>" class="card-link">Edit</a>
                                            <div class="form-check">
                                                <input class="form-check-input" required type="radio"
                                                    name="selectedAddress" value="<%= i %>" id="address<%= i %>"
                                                    onclick="updateSelectedAddressId(`<%=i%>`)">


                                                <label class="form-check-label" for="address<%= i %>">
                                                    Select Address
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <%}}else{%>

                                        <div class="alert alert-danger" role="alert">
                                            <h4 class="alert-heading">Attention!</h4>
                                            <p>Please add a delivery address to place the order.</p>
                                        </div>
                                        <%}%>

                    </div>


                    <div class="col-md-6">
                        <div class="order_review">
                            <div class="mb-20">
                                <h4>Your Orders</h4>
                            </div>
                            <div class="table-responsive order_table text-center">

                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th colspan="2">Product</th>
                                            <th>Total</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <% let totalAmount=0; cart.forEach((cartItem)=> {
                                            cartItem.products.forEach((product) => {
                                            let price = product.productId.salesPrice;
                                            totalAmount += parseFloat(price*product.quantity);

                                            });
                                            });

                                            %>

                                            <% cart.forEach((cartItem)=> { %>
                                                <% cartItem.products.forEach((product)=> { %>
                                                    <tr>
                                                        <td class="image product-thumbnail">
                                                            <img src="<%= product.productId.images[0] %>" alt="#">
                                                        </td>
                                                        <td>
                                                            <h5><a href="shop-product-full.html">
                                                                    <%= product.productId.name %>
                                                                </a></h5>
                                                            <span class="product-qty">x <%= product.quantity %></span>
                                                        </td>
                                                        <td>
                                                            <%= product.productId.salesPrice * product.quantity %>
                                                        </td>
                                                    </tr>
                                                    <% }); %>
                                                        <% }); %>

                                                            <tr>
                                                                <th>SubTotal</th>
                                                                <td class="product-subtotal" colspan="2">₹ <%=
                                                                        totalAmount%>
                                                                </td>
                                                                <input type="hidden" name="totalAmount"
                                                                    value="<%= totalAmount %>">
                                                            </tr>
                                                            <tr>
                                                                <th>Shipping</th>
                                                                <td colspan="2"><em>Free Shipping</em></td>
                                                            </tr>
                                                            <tr>
                                                                <th>AppliedCoupon</th>
                                                                <td colspan="2"><em><%=couponCode%></em></td>
                                                                <input type="hidden" name="couponCode" value="<%=couponCode%>">
                                                                <tr>
                                                                    <th>Discount</th>
                                                                    <td colspan="2"><em>₹<%=discount%></em></td>
                                                                    <input type="hidden" name="discount" value="<%= discount %>">
                                                                </tr>
                                                                <tr>
                                                                    <th>Total</th>
