    <main class="main-wrap">
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Wallet history</h2>
            </div>
            <div class="card" id="PDF">
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-9">
                            <header class="border-bottom mb-4 pb-4">
                                <div class="row">


                                   
                            </header>


                            <!-- card-header end// -->
                            <div class="table-responsive">
                                <table>
                                    <thead>
                                      <tr>
                                        <th>Transaction type</th>
                                        <th>Amount</th>
                                        <th>Description</th>
                                        <th>Date</th>
                                        <th class="text-end print-hidden">Balance</th>
                                      </tr>
                                    </thead>
                                    <tbody>
                                        <% if (wallet && wallet.transactions && wallet.transactions.length > 0) { %>
                                          <% wallet.transactions.forEach(transaction => { %>
                                            <tr>
                                              <td><%= transaction.type %></td>
                                              <td><%= transaction.amount %></td>
                                              <td><%= transaction.description %></td>
                                              <td><%= transaction.date.toISOString().split('T')[0] %></td>
                                              <td class="text-end print-hidden">
                                                <%= calculateBalance(transaction) %>
                                              </td>
                                            </tr>
                                          <% }); %>
                                        <% } else { %>
                                          <tr>
                                            <td colspan="5" class="text-center">No transactions available</td>
                                          </tr>
                                        <% } %>
                                      </tbody>
                                      
                                      
                                  </table>
                            </div> <!-- table-responsive.// -->
