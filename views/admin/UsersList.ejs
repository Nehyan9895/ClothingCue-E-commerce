    function confirmBlockUser(userId,act) {
        swal({
            title: "Are you sure?",
            text: "This action will toggle the user's status.",
            icon: "warning",
            buttons: ["Cancel", "Yes"],
            dangerMode: true,
        }).then((result) => {
            if (!result.isConfirmed) {
                window.location.href=`blockUser?userId=${userId}&act=${act}`;

            } else {
                console.log('ggggt');
            }
        });
        
    }
    
</script>
