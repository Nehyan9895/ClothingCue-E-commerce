            <td><%= users[i].name %></td>
            <td><%= users[i].email %></td>
            <td><%= users[i].mobile %></td>

            <td>
                <%
                    if (users[i].is_verified == 0) {
                %>
                <p class="badge rounded-pill alert-danger">Not active</p>
                <%
                    } else {
                %>
                <p class="badge rounded-pill alert-success">Active</p>
                <%
                    }
                %>
            </td>
            <td class="text-end">
            <input type="hidden" name="userId" value="<%= users[i]._id %>">
            <%if(users[i].is_verified==1){%>
             <button  onclick="confirmBlockUser(`<%=users[i]._id%>`,0)" class="btn btn-sm btn-brand rounded font-sm mt-15" >block</button>
                <%}else{%>
                    <button  onclick="confirmBlockUser(`<%=users[i]._id%>`,1)" class="btn btn-sm btn-brand rounded font-sm mt-15" >Unblock</button>
