    <main class="main-wrap">
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Sales Report</h2>
            </div>
            <div class="card" id="PDF">
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-9">
                            <header class="border-bottom mb-4 pb-4">
                                <div class="row">


                                    <div class="col-lg-9 col-12 d-flex justify-content-center ">
                                        <form action="" class="print-hidden" method="post">
                                            <label>Starting:</label>
                                            <input required type="date" name="start">


                                            <label>Ending:</label>
                                            <input type="date" name="end">
                                            <input required class="btn btn-primary" type="submit" value="Submit">
                                        </form>

                                    </div>
                            </header>


                            <!-- card-header end// -->
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>Transaction ID</th>
                                            <th>Paid</th>
                                            <th>Method</th>
                                            <th>Date</th>
                                            <th class="text-end print-hidden"> Action </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% let online=0 %>
                                            <% let fullAmount=0 %>
                                                <% if (orders.length> 0) { %>
                                                    <% for (let i=0; i < orders.length; i++) { %>
                                                        <% if (orders[i].orderStatus==='Delivered' ) { %>
                                                            <% if (orders[i].PaymentMethod=='online Payment' ) { online
                                                                +=1 } %>

                                                                <% const date=new Date(orders[i].orderDate) %>
                                                                    <% const
                                                                        formattedDate=date.toLocaleDateString('en-GB', {
                                                                        day: '2-digit' , month: '2-digit' ,
                                                                        year: 'numeric' }) %>

                                                                        <% fullAmount
                                                                            +=parseInt(orders[i].discountedAmount) %>
                                                                            <tr>
                                                                                <td><b>#<%= orders[i]._id %></b></td>
                                                                                <td>â‚¹<%= orders[i].discountedAmount %>
                                                                                        .00</td>
                                                                                <td>
                                                                                    <div class="icontext">
                                                                                        <!-- <img class="icon border" src="" alt="Payment"> -->
                                                                                        <span
                                                                                            class="text text-sm text-primary">
                                                                                            <%= orders[i].PaymentMethod
                                                                                                %>
                                                                                        </span>
                                                                                    </div>
                                                                                </td>
                                                                                <td>
                                                                                    <%= formattedDate %>
                                                                                </td>
                                                                                <td class="text-end print-hidden">
                                                                                    <a href="orderdetails?orderId=<%= orders[i]._id %>"
                                                                                        class="btn btn-sm btn-light font-sm rounded">Details</a>
                                                                                </td>
                                                                            </tr>
                                                                            <% } %>
                                                                                <% } %>
                                                                                    <% } %>
                                                                                        <style>
                                                                                            @media print {
                                                                                                .print-hidden {
                                                                                                    display: none;
                                                                                                }
                                                                                            }
                                                                                        </style>
                                    </tbody>

                                </table>
                            </div> <!-- table-responsive.// -->
                        </div> <!-- col end// -->
                        <aside class="col-lg-3">
                            <div class="box bg-light" style="min-height:80%">
                                <h6 class="mt-15">Transaction Details</h6>
                                <hr>
                                <% if (typeof start !=='undefined' && typeof end !=='undefined' ) { %>
                                    <h6 class="mb-0">From Date:</h6>
                                    <p>
                                        <%= start %>
                                    </p><br>
                                    <h6 class="mb-0">To Date:</h6>
                                    <p>
                                        <%= end %>
                                    </p><br>
                                    <% } else { %>
                                        <h6 class="mb-0">From Date:</h6>
                                        <p>Not specified</p><br>
                                        <h6 class="mb-0">To Date:</h6>
                                        <p>Not specified</p><br>
                                        <% } %>


                                            <h6 class="mb-0">Total Amount:</h6>
                                            <p>â‚¹<%=fullAmount%>.00</p><br>
                                            <span>
                                                <h6 class="mb-0">Total Orders:</h6>
                                            </span>
                                            <p>
                                                <%=orders.length%>
                                            </p><br>
                                            <h6 class="mb-0">Total Online:</h6>
                                            <p>
                                                <%=online%>
                                            </p><br>
                                            <h6 class="mb-0">Email:</h6>
                                            <p><template class="__cf_email__"
                                                    data-cfemail="9cefe9ececf3eee8dcf9e4fdf1ecf0f9b2fff3f1">[email&#160;protected]</template>
                                            </p><br>

                                            <p>Total Payment:</p>
                                            <p class="h4">â‚¹<%=fullAmount%>.00</p>
                                            <hr>
                                            <button class="btn btn-lightb print-hidden" id="print">Download
                                                Report</button>

                            </div>


