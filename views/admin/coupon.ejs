                                    <label for="product_name" class="form-label">Coupon Code</label>
                                    <input type="text" name="code" placeholder="Type here" class="form-control" id="product_name" />

                                </div>
                                <div class="mb-4">
                                    <label for="product_slug" class="form-label">Discount Amount</label>
                                    <input type="text" name="discount" placeholder="Type here" class="form-control" id="product_slug" />
                                </div>
                                <div class="mb-4">
                                    <label for="product_slug" class="form-label">Minimum Purchase</label>
                                    <input type="text" name="limit" placeholder="Type here" class="form-control" id="product_slug" />
                                </div>
                                <div >
                                    <label  class="form-label">Status</label>
                                    <select required name="status" class="form-select">
                                        <option>Active</option>
                                        <option>Inactive</option>
                                    </select>
                                </div>
                                <div class="mb-4">
                                    <label for="product_slug" class="form-label">Expiry Date</label>
                                    <input type="date" name="expirydate" placeholder="Type here" class="form-control" id="product_slug" />
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Description</label>
                                    <textarea  name="description" placeholder="Type here" class="form-control"></textarea>
                                </div>
                               
                                <div class="d-grid">
                                    <button type="submit" class="btn btn-primary">Create Coupon</button>
                                </div>
                            </form>
                            
                        </div>

<script>
function validateForm() {
    var code = document.forms["couponForm"]["code"].value.trim();
    var discount = document.forms["couponForm"]["discount"].value.trim();
    var limit = document.forms["couponForm"]["limit"].value.trim();
    var description = document.forms["couponForm"]["description"].value.trim();

    if (!code || !discount || !limit || !description) {
        alert("All fields must be filled.");
        return false;
    }

    if (isNaN(discount)) {
        alert("Discount must be a numeric value.");
        return false;
    }

    return true;
}
</script>
                        <div class="col-md-9">
                            <div class="table-responsive">
                                <table class="table table-hover" id="example">
                                    <thead>
                                        <tr>
                                            
                                        
                                            <th>Coupon Code</th>
                                           
                                            <th>Discount</th>
                                            <th>Above Orders</th>
                                            <th>Status</th>
                                            <th>Expiry Date</th>
                                            <th class="text-end">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%for(let i=0;i<coupons.length;i++){%>
                                        <tr>
                                            
                                            
                                            <td><h5 class="text-brand"><%=coupons[i].couponCode%></h5></td>
                                           
                                            <td><%=coupons[i].discount%></td>
                                            <td><%=coupons[i].limit%></td>
                                            <td>
                                            <%if(coupons[i].status){%>
                                                <span class="badge rounded-pill alert-success">Active</span>
                                                <%}else{%>
                                                <span class="badge rounded-pill alert-danger">Inactive</span>
                                                    <%}%>
                                            </td>
                                            <%const expiryDate = new Date(coupons[i].expiryDate);

                                            // Formatting the date to a desired format
                                            const formattedDate = expiryDate.toLocaleDateString('en-US', {
                                              year: 'numeric',
                                              month: 'short',
                                              day: 'numeric'
                                            });%>
                                            <td><%=formattedDate%></td>
                                            <td class="text-end">
                                                
                                                   
                                                        <a class="btn btn-brand" href="editcoupon?couponId=<%=coupons[i]._id%>">Edit</a>
                                                        
                                                  
                                                 <!-- dropdown //end -->
                                            </td>
                                        </tr>
                                        <%}%>

